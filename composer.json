{
    "name": "blockmurder/custom-upload-path",
    "description": "Adds custom upload path to a content type",
    "type": "bolt-extension",
    "version": "0.1",
    "keywords": [
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "blockmurder",
            "email": "info@blockmurder.ch"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\Blockmurder\\CustomUploadPath\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\Blockmurder\\CustomUploadPath\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-class": "Bolt\\Extension\\Blockmurder\\CustomUploadPath\\CustomUploadPathExtension"
    }
}
